// MARK: - Equal

// MARK: View == Anchor
@discardableResult
public func == (lhs: View, rhs: {{AnchorPairType}}) -> ConstraintPair {
    return lhs[keyPath: ...rhs] == rhs
}
// MARK: View == Expression
@discardableResult
public func == (lhs: View, rhs: {{AnchorPairType}}Expression) -> ConstraintPair {
    return lhs[keyPath: ...rhs.anchors] == rhs
}

// MARK: - LessThanOrEqual

// MARK: View <= Anchor
@discardableResult
