// MARK: [View] == Anchor
@discardableResult
public func == (lhs: [View], rhs: {{anchortype}}) -> Constraints {
    return lhs.map { $0 == rhs }
}

// MARK: [View] == Anchors
@discardableResult
public func == (lhs: [View], rhs: {{anchortype}}s) -> [Constraints] {
    return lhs.map { element in
        rhs.map({element == $0})
    }
}

