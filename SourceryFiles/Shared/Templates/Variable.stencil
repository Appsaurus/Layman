extension {{AnchorType}}: Variable {

    public typealias E = {{AnchorType}}Expression

    public func plus(_ constant: LayoutConstant) -> {{AnchorType}}Expression {
        return {{AnchorType}}Expression(anchor: self, configuration: .constant(constant))
    }

    public func times(_ multiplier: LayoutMultiplier) -> {{AnchorType}}Expression {
        return {{AnchorType}}Expression(anchor: self, configuration: .multiplier(multiplier))
    }
}
