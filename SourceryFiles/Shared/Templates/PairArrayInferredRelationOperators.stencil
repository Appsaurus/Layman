// MARK: [View] == Anchor
@discardableResult
public func == (lhs: [View], rhs: {{AnchorPairType}}) -> [ConstraintPair] {
    return lhs.map { $0 == rhs }
}

// MARK: [View] == Anchors
@discardableResult
public func == (lhs: [View], rhs: {{AnchorPairType}}s) -> [[ConstraintPair]] {
    return lhs.map { element in
        rhs.map({element == $0})
    }
}

