@discardableResult
public func + (lhs: {{AnchorType}}, rhs: LayoutConstant) -> {{AnchorType}}Expression {
    return LayoutExpression(anchor: lhs, configuration: LayoutConfiguration(constant: rhs))
}

@discardableResult
public func - (lhs: {{AnchorType}}, rhs: LayoutConstant) -> {{AnchorType}}Expression {
    return LayoutExpression(anchor: lhs, configuration: LayoutConfiguration(constant: -rhs))
}

@discardableResult
public func * (lhs: {{AnchorType}}, rhs: LayoutConstant) -> {{AnchorType}}Expression {
    return LayoutExpression(anchor: lhs, configuration: LayoutConfiguration(multiplier: rhs))
}

@discardableResult
public func / (lhs: {{AnchorType}}, rhs: LayoutConstant) -> {{AnchorType}}Expression {
    return lhs * (1.0 / rhs)
}

@discardableResult
public func / (lhs: {{AnchorType}}Expression, rhs: LayoutConstant) -> {{AnchorType}}Expression {
    return lhs.with(multiplier: (1.0 / rhs))
}

