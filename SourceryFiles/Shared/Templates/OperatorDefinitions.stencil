precedencegroup RelationPrecedence {
    associativity: none
    higherThan: ComparisonPrecedence
    lowerThan: AdditionPrecedence
}

{% if argument.EqualityOperator != '==' %}
infix operator {{argument.EqualityOperator}} : RelationPrecedence
{% endif %}

{% if argument.LessThanOrEqualToOperator != '<=' %}
infix operator {{argument.LessThanOrEqualToOperator}} : RelationPrecedence
{% endif %}

{% if argument.GreaterThanOrEqualToOperator != '>=' %}
infix operator {{argument.GreaterThanOrEqualToOperator}} : RelationPrecedence
{% endif %}

