{% for EquationProtocol in argument.EquationProtocols %}
{% set Protocol %}{{argument.EquationProtocols[EquationProtocol]}}{% endset %}
{% set RightHandTypes %}{{argument.EquationProtocols[EquationProtocol]["RightHandTypes"]}}{% endset %}
{% set RelationshipType %}{{argument.EquationProtocols[EquationProtocol]["RelationshipType"]}}{% endset %}
