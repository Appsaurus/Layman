// MARK: - Equal

// MARK: Anchor == Anchor Array
@discardableResult
public func == (lhs: {{anchortype}}Pair, rhs: {{anchortype}}Pairs) -> [ConstraintPair] {
    return rhs.map({lhs == $0})
}

// MARK: Anchor Array == Anchor
@discardableResult
public func == (lhs: {{anchortype}}Pairs, rhs: {{anchortype}}Pair) -> [ConstraintPair] {
    return lhs.map({$0 == rhs})
}

// MARK: Anchor Array == Anchor Array
@discardableResult
public func == (lhs: {{anchortype}}Pairs, rhs: {{anchortype}}Pairs) -> [[ConstraintPair]] {
    return lhs.map { element in
        rhs.map { element == $0 }
    }
}

