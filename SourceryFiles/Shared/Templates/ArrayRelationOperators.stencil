// MARK: - Equal

// MARK: Anchor == Anchor Array
@discardableResult
public func == (lhs: {{AnchorType}}, rhs: {{AnchorType}}s) -> Constraints {
    return rhs.map({lhs == $0})
}

// MARK: Anchor Array == Anchor
@discardableResult
public func == (lhs: {{AnchorType}}s, rhs: {{AnchorType}}) -> Constraints {
    return lhs.map({$0 == rhs})
}

// MARK: Anchor Array == Anchor Array
@discardableResult
public func == (lhs: {{AnchorType}}s, rhs: {{AnchorType}}s) -> [Constraints] {
    return lhs.map { element in
        rhs.map { element == $0 }
    }
}

